        <sqlMapGenerator targetPackage="mapper.system" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成Mapper接口 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.netmarch.web.system.mapper" targetProject="src/main/java">
